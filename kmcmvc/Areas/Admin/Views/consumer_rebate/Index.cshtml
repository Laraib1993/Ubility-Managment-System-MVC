@model IEnumerable<kmcmvc.Areas.Admin.Data.consumer_rebate>

@{
    ViewBag.Title = "Rebate Consumers";
}

<div class="pcoded-inner-content">
    <!-- Main-body start -->
    <div class="main-body">
        <div class="page-wrapper">
            <!-- Page-body start -->
            <div class="page-body">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="card">
                            <div class="card-header table-card-header">
                                <h5>MUCT KMC Rebate Consumers List</h5>

                            </div>
                            <div class="card-block">
                                <span></span>
                                <div class="dt-responsive table-responsive">
                                    <table id="dt-post" class="table table-striped table-bordered nowrap">
                                        <thead>
                                            <tr>
                                                <th>Consumer No</th>
                                                <th>Naame</th>
                                                <th>Category</th>
                                                <th>Area</th>
                                                <th>Before Rebate</th>
                                                <th>After Rebate</th>
                                                <th>Issued By</th>
                                                <th>Issued On</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        <tbody>
                                            @foreach (var item in Model)
                                            {

                                            <tr>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.consumer_no)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.consumer_name)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.category)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.area)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.outstanding_Arrears)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.after_rebate)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.createdby)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.createdon)
                                                </td>
                                                <td>
                                                    @Html.ActionLink("Details", "Details", new { id = item.sno })
                                                </td>
                                            </tr>



                                            }
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                                <th>Consumer No</th>
                                                <th>Naame</th>
                                                <th>Category</th>
                                                <th>Area</th>
                                                <th>Before Rebate</th>
                                                <th>After Rebate</th>
                                                <th>Issued By</th>
                                                <th>Issued On</th>
                                                <th>Action</th>
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



